@model List<Category>

<h1>Category list</h1>

<table class="table table-bordered table-striped">
	<thead>
	<tr>
		<th>Category Name</th>
		<th>Category Id</th>
	</tr>
	</thead>
	<tbody>
	
		@foreach (var obj in Model.OrderBy(o=>o.CategoryId))
		{
			<tr>
				<td>@obj.Name</td>
				<td>@obj.CategoryId</td>
			</tr>
			
		}

	</tbody>
</table>

